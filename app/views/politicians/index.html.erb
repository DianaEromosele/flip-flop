<div class="container heading-container">
  <h2 class="heading-text">Politicians on Twitter </h2>

<ul class="list-unstyled">
  <% @politicians.each do |politician| %>
    <li><%= link_to image_tag(politician.image, class: "politician-image"), politician_path(politician)  %><%= politician.name %></li>
  <% end %>
</ul>

