<div class="container heading-container">
  <h2 class="heading-text">trending politicians</h2>

<ul class="list-unstyled">
  <% @politicians.each do |politician| %>
    <li class="politician-name"><%= link_to image_tag(politician.image, class: "politician-image")+politician.name, politician_path(politician) %></li>
  <% end %>
</ul>

